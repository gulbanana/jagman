{
	"name": "@gulbanana/jagman",
	"version": "0.1.0",
	"author": {
		"name": "Thomas Castiglione"
	},
	"type": "module",
	"description": "Dashboard for managing multiple LLM coding agents across repositories using Jujutsu workspaces",
	"license": "AGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/gulbanana/jagman.git"
	},
	"homepage": "https://github.com/gulbanana/jagman",
	"bugs": "https://github.com/gulbanana/jagman/issues",
	"keywords": [
		"llm",
		"agent",
		"coding-agent",
		"claude",
		"opencode",
		"jujutsu",
		"dashboard",
		"sveltekit"
	],
	"bin": {
		"jagman": "bin/jagman.js"
	},
	"napi": {
		"binaryName": "libgg",
		"targets": [
			"x86_64-pc-windows-msvc",
			"aarch64-apple-darwin",
			"x86_64-unknown-linux-gnu"
		]
	},
	"optionalDependencies": {
		"@gulbanana/jagman-win32-x64-msvc": "0.0.5",
		"@gulbanana/jagman-darwin-arm64": "0.0.5",
		"@gulbanana/jagman-linux-x64-gnu": "0.0.5"
	},
	"files": [
		"build/**",
		"bin/**",
		"native/**",
		"index.js",
		"index.d.ts",
		"*.node"
	],
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"build:native": "napi build --release --platform --esm",
		"build:native:debug": "napi build --platform --esm",
		"dev": "vite dev",
		"build": "npm run build:native && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"prepublishOnly": "npm run build",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@napi-rs/cli": "^3.0.0",
		"@playwright/test": "^1.58.1",
		"@sveltejs/adapter-node": "^5.5.3",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/better-sqlite3": "^7.6.13",
		"@types/feather-icons": "^4.29.4",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.18",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"playwright": "^1.58.1",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	},
	"dependencies": {
		"@catppuccin/palette": "^1.7.1",
		"@github/copilot-sdk": "^0.1.29",
		"@opencode-ai/sdk": "^1.2.10",
		"better-sqlite3": "^12.6.2",
		"env-paths": "^4.0.0",
		"feather-icons": "^4.29.2",
		"libgg": "file:native",
		"modern-normalize": "^3.0.1"
	}
}
