		"playwright": "^1.58.1",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	},
	"dependencies": {
		"@catppuccin/palette": "^1.7.1",
		"feather-icons": "^4.29.2",
